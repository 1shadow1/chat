OPENAI_API_KEY=
ALLOWED_ORIGINS=*

# 语音克隆服务配置（已禁用；保留占位以兼容旧部署，不再生效）
# VOICE_CLONE_BASE_URL=
# VOICE_CLONE_API_KEY=
# VOICE_USE_MOCK=0

# 日志配置（可选）
# 设为 1 开启文件持久化；默认仅输出到控制台
LOG_TO_FILE=1
# 文件路径（默认优先 /srv/chat/log/chat.log，其次 /srv/chat/logs/chat.log）
LOG_FILE_PATH=/srv/chat/log/chat.log
# 日志等级：DEBUG/INFO/WARNING/ERROR/CRITICAL（默认 INFO）
LOG_LEVEL=INFO
LOG_INCLUDE_INPUT=0
LOG_INCLUDE_OUTPUT=none
LOG_CONTENT_MAX_CHARS=1000
LOG_REDACT_ENABLED=1
SESSION_LOG_ENABLED=0
SESSION_LOG_BASE_DIR=/srv/chat/log
# 以下变量已不再使用（行流式推送功能已移除）
# VOICE_LINE_BASE_URL=
# VOICE_LINE_USE_MOCK=1
# VOICE_LINE_DIR=/srv/chat/log